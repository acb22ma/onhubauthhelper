{
  "name": "OnHubAuthHelper",
  "version": "2.0.0",
  "manifest_version": 3,

  "permissions": [
    "identity"
  ],

  "host_permissions": [
    "https://accounts.google.com/*",
    "https://oauth2.googleapis.com/*"
  ],

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_popup": "popup.html"
  }
}
